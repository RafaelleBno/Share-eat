package com.example.MyApplication; 
import android.content.Intent;
import android.os.Bundle;
import android.view.View;
import android.widget.ImageView;
import androidx.appcompat.app.AppCompatActivity;

public class FavorisActivity extends AppCompatActivity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_favoris); // adapte si ton fichier XML a un autre nom

        ImageView backArrow = findViewById(R.id.back_arrow); // Assure-toi d'avoir mis un id

        backArrow.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(FavorisActivity.this, Profil.class);
                startActivity(intent);
                finish(); // Optionnel : ferme l'activit√© actuelle
            }
        });
    }
}
